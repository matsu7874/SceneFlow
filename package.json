      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.5.0",
    "d3": "^7.9.0",
